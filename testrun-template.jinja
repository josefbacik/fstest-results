<html>
<head>
	<title>fstests results</title>
	<link rel="stylesheet" href="../../../../style.css"/>
</head>
<body>
<div>
  <div>
    <table class="summary">
      <tr><th colspan="2">Summary</th></tr>
      <tr><th>Status</th><th>Count</th></tr>
      <tr><td>Pass</td><td>{{ run.passes|length }}</td></tr>
      <tr><td>Fails</td><td>{{ run.fails|length }}</td></tr>
      <tr><td>Not Run</td><td>{{ run.notruns|length }}</td></tr>
    </table>
  </div>
  <div>
    <table class="results_output">
      <tr>
        <th class="failing" colspan="4">Failures ({{ run.fails|length }} total)</th>
      </tr>
      <tr>
        <th>Name</th><th>out.bad</th><th>dmesg</th><th>Date</th>
      </tr>
      {% for t in run.fails %}
        <tr>
          <td>{{ t.name }}</td>
          <td><a href="{{ t.name + ".out.bad" }}">{{ t.name + ".out.bad" }}</a></td>
          {% if run.dmesg_output(t.name) %}
            <td><a href="{{ t.name + ".dmesg" }}">{{ t.name + ".dmesg" }}</a></td>
          {% else %}
            <td>No dmesg output</td>
          {% endif %}
          <td>{{ run.date }}</td>
        </tr>
      {% endfor %}
    </table>
    <table class="results_time">
      <tr>
        <th class="passing" colspan="3">Passing ({{ run.passes|length }} total)</th>
      </tr>
      <tr>
        <th>Name</th><th>Time spent</th><th>Date</th>
      </tr>
      {% for t in run.passes %}
        <tr><td>{{ t.name }}</td><td>{{ run.date }}</td></tr>
      {% endfor %}
    </table>
    <table class="results">
      <tr>
        <th class="notrun" colspan="2">Notruns ({{ run.fails|length }} total)</th>
      </tr>
      <tr>
        <th>Name</th><th>Date</th>
      </tr>
      {% for t in run.notruns %}
        <tr><td>{{ t.name }}</td><td>{{ t.date }}</td></tr>
      {% endfor %}
    </table>
  </div>
</div>
</body>
</html>
